{
  "name": "mmousset/pimcore-classification-store-importer",
  "description": "Pimcore bundle for Classificationstore import and export",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Mousset Maxime",
      "email": "maxime.mousset@arhis.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "ext-intl" : "*",
    "pimcore/pimcore": ">=11.4"
  },
  "autoload": {
    "psr-4": {
      "Mousset\\ClassificationstoreBundle\\": "src/ClassificationstoreBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mousset\\ClassificationstoreBundle\\Test\\": "tests/unit/tests/ClassificationstoreBundle/"
    },
    "classmap": [
      "app/AppKernel.php"
    ]
  },

  "extra": {
    "pimcore": {
      "bundles": [
        "Mousset\\ClassificationstoreBundle\\ClassificationstoreBundle"
      ]
    }
  }
}
